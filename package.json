{
  "$schema": "https://json.schemastore.org/package",
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev": "turbo run dev:next",
    "build:next": "next build",
    "build": "turbo run build:next",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "next": "^14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "shell-quote": "^1.8.1",
    "tailwindcss": "3.3.5",
    "turbo": "^1.10.16",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  }
